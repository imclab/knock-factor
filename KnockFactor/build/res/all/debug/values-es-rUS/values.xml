    <string name="timesync_sync_now_progress_dialog_title" msgid="7008645503609657769">"Sincronizar ahora"</string>
    <string name="timesync_sync_now_screen_title" msgid="3545642903720968544">"Sincronizar ahora"</string>
    <string name="timesync_sync_now_time_already_correct_dialog_details" msgid="1783578393804196806">"No se ajustó la hora del reloj interno de Google Authenticator porque, al parecer, ya se sincronizó con los servidores de Google."</string>
