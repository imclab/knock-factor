    <string name="timesync_preference_screen_title" msgid="3022961447302189451">"Urni popravek za kode"</string>
    <string name="timesync_sync_now_connectivity_error_dialog_details" msgid="7427576990969917093">"Težava pri povezovanju z Googlovim strežnikom. Preverite omrežne nastavitve in poskusite znova."</string>
