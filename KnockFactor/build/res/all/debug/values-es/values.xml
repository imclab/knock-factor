    <string name="timesync_preference_screen_title" msgid="3022961447302189451">"Corrección de la hora para los códigos"</string>
    <string name="timesync_sync_now_connectivity_error_dialog_details" msgid="7427576990969917093">"Se ha producido un error al establecer contacto con el servidor de Google. Comprueba tu configuración de red e inténtalo de nuevo."</string>
